{
  "name": "propman",
  "version": "1.0.2",
  "description": "etcd editor",
  "homepage": "https://github.com/helkatz/propman",
  "author": {
    "name": "Helmut Katz",
    "email": "hkatz.develop@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 7",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "private": false,
  "files": [
    "build",
    "*.js",
    "public"
  ],
  "xbuild": {
    "appId": "com.hk-develop.propman",
    "productName": "etcd manager",
    "target": "NSIS",
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "win": {
      "publish": [
        "github"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "helkatz",
        "repo": "propman"
      }
    ]
  },
  "scripts": {
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve --port 14202",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:14202/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "publish": "electron-builder --publish always",
    "pack": "electron-builder --config=electron-builder.yaml --dir",
    "dist-old": "electron-builder",
    "release": "build",
    "package": "electron-packager . --platform=win32 --arch=x64 --ignore=src1 --ignore=node-modules --overwrite --asar",
    "dist": "build -w --x64"
  },
  "dependencies": {
    "@types/lodash": "^4.14.123",
    "angular2-query-builder": "^0.4.2",
    "chart.js": "^2.9.3",
    "deep-object-diff": "^1.1.0",
    "editor.js": "^0.1.0",
    "electron-updater": "^4.0.6",
    "http-server": "^0.11.1",
    "jqwidgets-ng": "^8.3.2",
    "lodash": "^4.17.11",
    "lodash.isequal": "^4.5.0",
    "mysql": "^2.17.1",
    "node-sass": "^4.13.0",
    "quill": "^1.3.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.20",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^7.3.5",
    "@angular/cli": "8.3.20",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "8.2.14",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "8.2.14",
    "@angular/http": "7.2.1",
    "@angular/language-service": "8.2.14",
    "@angular/material": "^7.3.5",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/jasmine": "3.3.7",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "10.12.18",
    "asar": "^1.0.0",
    "bootstrap": "^4.3.1",
    "codelyzer": "4.5.0",
    "conventional-changelog-cli": "2.0.11",
    "core-js": "^2.6.5",
    "core.js": "^0.4.2",
    "electron": "^4.1.0",
    "electron-builder": "20.38.5",
    "electron-reload": "1.4.0",
    "font-awesome": "^4.7.0",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "jquery": "^3.3.1",
    "jsonpath": "^1.0.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "npm-run-all": "4.1.5",
    "popper.js": "^1.14.7",
    "primeicons": "^1.0.0",
    "primeng": "^8.0.4",
    "protractor": "5.4.2",
    "rxjs": "6.5.3",
    "ts-node": "7.0.1",
    "tslint": "5.12.1",
    "typescript": "3.5.3",
    "update-electron-app": "^1.3.0",
    "wait-on": "3.2.0",
    "webdriver-manager": "12.1.1",
    "zone.js": "0.9.1"
  }
}
